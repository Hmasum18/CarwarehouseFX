<AnchorPane prefHeight="210.0" prefWidth="508.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="github.hmasum18.carshowroomfrontend.view.controller.DeleteCarController">
   <children>
      <Label layoutX="191.0" layoutY="6.0" text="Are you sure?" textFill="#f56162">
         <font>
            <Font name="Comic Sans MS Bold" size="38.0" />
         </font>
      </Label>
      <ImageView fx:id="mCarIV" fitHeight="112.0" fitWidth="126.0" layoutX="42.0" layoutY="33.0" pickOnBounds="true" preserveRatio="true">
